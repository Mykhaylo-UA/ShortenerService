@model ShortnerService.Models.Link

<table class="table">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">IP</th>
            <th scope="col">Country</th>
            <th scope="col">City</th>
            <th scope="col">Browser</th>
            <th scope="col">OS</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Statistic s in Model.Statistics)
        {
            <tr>
                <th scope="row">@s.DateTime</th>
                <th>@s.IP</th>
                <td>@s.Country</td>
                <td>@s.City</td>
                <td>@s.Browser</td>
                <td>@s.OS</td>
            </tr>
        }
    </tbody>
</table>